{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "module": "esnext",
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es2015",
    "lib": [
      "es2018",
      "dom"
    ]
  },
  "angularCompilerOptions": {
    "fullTemplateTypeCheck": true,
    "strictInjectionParameters": true
  },
  "paths": {
    "@env/*": [
      "src/environments/*"
    ],
    "@app-core/*": [
      "src/app/core/*"
    ],
    "@app-auth/*": [
      "src/app/authentication/*"
    ],
    "@app-shared/*": [
      "src/app/shared/*"
    ],
    "@app-cv/*": [
      "src/app/cv/*"
    ],
    "@app-settings/*": [
      "src/app/settings/*"
    ],
  }
}
